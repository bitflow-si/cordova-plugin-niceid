<resources>

    <style name="barcode_top_txt">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#009ddb</item>
        <item name="android:background">#f6f6f6</item>
        <item name="android:letterSpacing">-0.09</item>
        <item name="android:lineSpacingExtra">6sp</item>
    </style>

    <style name="barcode_bottom_txt">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#1b2946</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
    </style>

    <style name="barcode_blue_txt">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#009ddb</item>
        <item name="android:letterSpacing">-0.015</item>
        <item name="android:lineSpacingExtra">7sp</item>
    </style>

    <style name="login_black_double_txt">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#102542</item>
        <item name="android:letterSpacing">-0.015</item>
        <item name="android:lineSpacingExtra">7sp</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="save_btn">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:padding">12dp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">16sp</item>
    </style>

    <style name="EditText" parent="Widget.AppCompat.EditText">
        <item name="android:padding">12dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColorHint">#C3C3CB</item>
        <item name="android:textColor">#424B55</item>
        <item name="android:textSize">13sp</item>
        <item name="android:background">@drawable/background_edit_text</item>
    </style>

</resources>